testid is op Idstr _Args_ {
  NONLOCAL spacelost;
  IDstr := string Idstr;
  write link 'starting ' Idstr ;
  write status;
  freespace := first status;
  FOR Arg WITH _Args_ DO
     { % put in a block so that locals will be freed;
     write link 'trying ' Idstr ' on ' Arg;
     Vals gets execute Arg;
     if isfault Vals THEN
        write link Arg ' evaluated to ' (string Vals);
     else
         write link 'testing id ' Idstr;
         fn := parse scan Idstr;
         for Val with Vals DO
      write display Val;
            Resa := fn apply Val;
     write Resa;
            If not Resa THEN
               write 'failed on arg:'
               write display Val;
            endif;
         endfor;
     endif;
     write link 'completed ' Arg;
     };
  ENDFOR;
}
